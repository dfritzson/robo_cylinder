<launch>
  <arg name="base_link" default="base_link" doc="Enter argument [base_link:your_desired_link]"/>
  <node name="robo_control" pkg="robo_cylinder" type="robo_cylinder_services.py" respawn="true" />
  <node name="current_pos" pkg="robo_cylinder" type="pos_publisher.py" respawn="true" />
  <node name="car_link_broadcaster" pkg="robo_cylinder" type="car_tf_broadcaster.py" respawn="true" >
    <param name="robo_base" type="string" value="$(arg base_link)" />
  </node>
</launch>
